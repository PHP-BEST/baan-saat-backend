{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "developer": true
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "preview": {
    "env": {
      "NODE_ENV": "development"
    }
  }
}
